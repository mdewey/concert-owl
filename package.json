    "upcoming": "serverless invoke local -f pushDailySummary",
    "node:weekly": "node --env-file=.env scripts/weekly.js",
    "node:upcoming": "node scripts/upcoming.js",
    "node:ai": "node --env-file=.env scripts/test.js",
    "help": "serverless help",
    "test": "jest",
    "test-coverage": "jest --coverage",
